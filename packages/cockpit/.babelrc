{
  "presets": ["@babel/preset-env"],
  "plugins": [
    "add-module-exports",
    "ramda",
    "transform-object-rest-spread"
  ],
  "env": {
    "test": {
      "plugins": ["@babel/plugin-transform-runtime"],
    },
    "production": {
      "ignore": [
        "*.test.js",
        "**/*.test.js",
        "**/**/*.test.js",
      ],
      "sourceMaps": true,
    }
  }
}
